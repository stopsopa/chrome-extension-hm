    "style:fix": "yarn prettier --config prettier.config.mjs --write .",
    "style:list": "yarn prettier --config prettier.config.mjs --list-different ."
  },
  "dependencies": {},
  "devDependencies": {
    "c8": "^10.1.3",
    "prettier": "^3.6.2"
  }
}

